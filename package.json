{
  "name": "rest-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run build && node ./public/bin/www",
    "dev-start": "npm run build && nodemon ./public/bin/www",
    "watch": "babel --watch src --out-dir public --source-maps inline --copy-files",
    "build": "babel src --out-dir public --source-maps inline --copy-files"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ],
    "plugins": [
      "transform-flow-strip-types",
      "add-module-exports"
    ]
  },
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^6.0.2",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-flow-strip-types": "^6.7.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.7.2",
    "body-parser": "~1.17.1",
    "connect-flash": "^0.1.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.3",
    "express": "~4.15.2",
    "jsonwebtoken": "^7.4.1",
    "mongoose": "^4.11.0",
    "mongoose-currency": "^0.2.0",
    "morgan": "~1.8.1",
    "nodemailer": "^4.0.1",
    "nodemon": "^1.11.0",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth2": "^0.1.6",
    "passport-linkedin-oauth2": "^1.5.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^4.0.0",
    "passport-twitter": "^1.0.4",
    "serve-favicon": "~2.4.2",
    "watson-developer-cloud": "^2.34.0"
  },
  "devDependencies": {
    "nodemon": "^1.11.0"
  }
}
